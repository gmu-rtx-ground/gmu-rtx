<!-- 
Odometry config order
[x,         y,          z,        # Position
 roll,      pitch,      yaw,      # Orientation
 x vel,     y vel,      z vel,    # Linear Velocity   
 roll vel,  pitch vel,  yaw vel,  # Angular Velocity
 x accel,   y accel,    z accel ] # Linear acceleration


 -->

<launch>
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization_node" respawn="true">
        
        <!-- <param name="map_frame" value="map"/> -->
        <!-- <param name="odom0" value"/losi/odometry/filtered"/> -->

        <param name="frequency" value="30"/>
        <param name="sensor_timeout" value="0.1"/>
        <param name="two_d_mode" value="false"/>
        <param name="transform_time_offset" value="0.0"/>
        <param name="print_diagnostics" value="true"/>
        <param name="map_frame" value="map"/>
        <param name="odom_frame" value="odom"/>
        <param name="base_link_frame" value="base_link"/>
        <param name="world_frame" value="odom"/>

        <param name="imu0" value="/losi/witmotion_imu/imu"/>
        <param name="imu0_frequency" value="200"/>
        <param name="imu0_differential" value="false"/>
        <param name="imu0_relative" value="true"/>
        <param name="imu0_queue_size" value="1000"/>
        <param name="imu0_nodelay" value="false"/>
        <rosparam param="imu0_config">[false , false , false ,
                                       true  , true  , true  ,
                                       false , false , false ,
                                       true  , true  , true  ,
                                       false , false , false ]</rosparam> 
        <param name="gps0" value="/losi/f9p_rover/fix"/>
        <param name="gps0_differential" value="false"/>
        <param name="gps0_queue_size" value="5"/>
        <param name="gps0_nodelay" value="false"/>
        <rosparam param="gps0_config">[true  , true  , false ,
                                       false , false , false ,
                                       false , false , false ,
                                       false , false , true  ,
                                       false , false , false ]</rosparam>        
        
    </node>
</launch>